<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="#FF595959" />
    <SolidColorBrush x:Key="EdgeBorderBrush" Color="Gray" />

    <FontFamily x:Key="WindowFontFamily"/>
    <FontStyle x:Key="WindowFontStyle" />
    <FontWeight x:Key="WindowFontWeight"/>

    <BitmapSource x:Key="WowIcon">Images/Wow.ico</BitmapSource>
    <BitmapSource x:Key="WallPaper50">Images/wallpaper50.jpg</BitmapSource>
    <BitmapSource x:Key="WallPaper60">Images/wallpaper60.jpg</BitmapSource>
    <BitmapSource x:Key="WallPaper70">Images/wallpaper70.jpg</BitmapSource>
    <BitmapSource x:Key="FileImage">Images/File.png</BitmapSource>
    <BitmapSource x:Key="FolderImage">Images/Folder.png</BitmapSource>

</ResourceDictionary>